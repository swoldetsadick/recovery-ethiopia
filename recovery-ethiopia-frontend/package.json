{
  "name": "recovery-ethiopia-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:de": "ng serve --configuration=de --port 4200",
    "start:en": "ng serve --port 4201",
    "start:fr": "ng serve --configuration=fr --port 4202",
    "start:it": "ng serve --configuration=it --port 4203",
    "start:ar": "ng serve --configuration=ar --port 4204",
    "build:production": "ng build --configuration=production --localize --base-href /recovery-ethiopia-frontend && npm run docs:gen",
    "test": "ng test",
    "lint:": "ng lint",
    "e2e": "ng e2e",
    "trans": "ng extract-i18n --output-path='translations'",
    "docs:gen": "npx compodoc -p tsconfig.app.json --theme 'material'  -n 'Recovery Ethiopia' -d ./documentation --language 'en-US' src",
    "docs:serve": "npx compodoc -p tsconfig.app.json --theme 'material'  -n 'Recovery Ethiopia' -d ./documentation --port 4205 --language 'en-US' src -s",
    "preinstall": "npm-force-resolutions",
    "postinstall": "ngcc"
  },
  "resolutions": {
    "ajv": "7.2.4",
    "ajv-errors": "3.0.0",
    "ansi-html": "https://registry.npmjs.org/ansi-html-community/-/ansi-html-community-0.0.8.tgz",
    "ansi-regex": "5.0.1",
    "glob-parent": "5.1.2",
    "json-schema": "0.4.0",
    "set-value": "4.1.0"
  },
  "private": true,
  "type": "module",
  "dependencies": {
    "@angular/animations": "13.0.2",
    "@angular/cdk": "13.0.2",
    "@angular/common": "13.0.2",
    "@angular/compiler": "13.0.2",
    "@angular/core": "13.0.2",
    "@angular/forms": "13.0.2",
    "@angular/material": "13.0.2",
    "@angular/platform-browser": "13.0.2",
    "@angular/platform-browser-dynamic": "13.0.2",
    "@angular/router": "13.0.2",
    "d3": "6.7.0",
    "hammerjs": "2.0.8",
    "highcharts": "9.3.1",
    "moment": "2.29.1",
    "moment-timezone": "0.5.34",
    "rxjs": "6.6.7",
    "socket.io-client": "4.4.0",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.0.3",
    "@angular/cli": "13.0.2",
    "@angular/compiler-cli": "13.0.2",
    "@angular/localize": "13.0.2",
    "@compodoc/compodoc": "1.1.15",
    "@types/d3": "6.7.5",
    "@types/highcharts": "7.0.0",
    "@types/jasmine": "3.10.2",
    "@types/moment": "2.13.0",
    "@types/moment-timezone": "0.5.30",
    "@types/node": "12.20.37",
    "@types/socket.io-client": "1.4.36",
    "codelyzer": "6.0.2",
    "is-primitive": "3.0.1",
    "ajv-keywords": "5.1.0",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "6.3.9",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "npm-force-resolutions": "0.0.10",
    "protractor": "7.0.0",
    "ts-node": "8.3.0",
    "tslint": "6.1.3",
    "typescript": "4.4.4"
  }
}
